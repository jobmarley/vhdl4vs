<ProjectSchemaDefinitions
    xmlns="http://schemas.microsoft.com/build/2009/properties">

    <ContentType
        Name="VHDLSource"
        DisplayName="VHDL Source file"
        ItemType="VHDLSource">
    </ContentType>
    <ContentType
        Name="VerilogSourceFile"
        DisplayName="Verilog Source file"
        ItemType="VerilogSource">
    </ContentType>

        <!--<ContentType
        Name="Script"
        DisplayName="Script file"
        ItemType="Script">
    </ContentType>-->

    <ContentType
        Name="Text"
        DisplayName="Text file"
        ItemType="None">
    </ContentType>

    <ItemType Name="VHDLSource" DisplayName="VHDL source file"/>
    <ItemType Name="VerilogSource" DisplayName="Verilog source file"/>
    <!--<ItemType Name="Script" DisplayName="Script"/>-->
    <ItemType Name="None" DisplayName="Does not participate in build"/>

    <FileExtension Name=".vhd" ContentType="VHDLSource"/>
    <FileExtension Name=".v" ContentType="VerilogSource"/>
    <!--<FileExtension Name=".vbs" ContentType="Script"/>
    <FileExtension Name=".jS" ContentType="Script"/>-->
    <FileExtension Name=".txt" ContentType="Text"/>
</ProjectSchemaDefinitions>
